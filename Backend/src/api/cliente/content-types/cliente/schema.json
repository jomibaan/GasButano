{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "singularName": "cliente",
    "pluralName": "clientes",
    "displayName": "Cliente"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombres": {
      "type": "string",
      "required": false
    },
    "apellidos": {
      "type": "string",
      "required": false
    },
    "telefono": {
      "type": "string",
      "minLength": 10,
      "maxLength": 10,
      "required": true,
      "unique": true
    },
    "estado": {
      "type": "boolean",
      "default": true
    },
    "servicios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::servicio.servicio",
      "mappedBy": "cliente"
    },
    "domicilios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::domicilio.domicilio",
      "mappedBy": "cliente"
    }
  }
}
